<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>

    <template id="asset_frontend" inherit_id="website.assets_frontend" name="Theme Hub Frontend">
      <xpath expr="." position="inside">
        <!-- Libraries CSS Files -->
        <link href="/sah_technomar/static/src/lib/animate/animate.css" rel="stylesheet" />
        <link rel="preconnect" href="https://fonts.gstatic.com" />
        <link href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,300;0,400;0,500;0,600;0,800;0,900;1,300;1,400;1,500;1,600;1,800;1,900&amp;family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&amp;display=swap" rel="stylesheet" />
        <!-- stylesheet -->
        <link href="/sah_technomar/static/src/css/style.css" rel="stylesheet" type="text/css"/>
        <link href="/sah_technomar/static/src/css/responsive.css" rel="stylesheet" type="text/css"/>

        <script src="/sah_technomar/static/src/js/snippets.js" type="text/javascript" ></script>
        <script src="/sah_technomar/static/src/lib/wow/wow.min.js" type="text/javascript" ></script>
        <script src="/sah_technomar/static/src/js/scripts.js" type="text/javascript" ></script>


      </xpath>
    </template>

    <template id="assets_snippets_options" inherit_id="website.assets_editor" name="Snippets Options">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/sah_technomar/static/src/js/snippets_options.js" />
        </xpath>
    </template>

  </data>
</odoo>