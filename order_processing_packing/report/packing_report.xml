<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_report_standard_packagelist" model="ir.actions.report">
            <field name="name">Standard package list</field>
            <field name="model">order.processing</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">order_processing_packing.report_standard_packagelist</field>
